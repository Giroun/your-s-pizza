generator client {
  provider = "prisma-clien-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  fullName  String
  email     String
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updetedAt
}
